; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Fusion Settings"
config/features=PackedStringArray("4.5")
config/icon="res://icon.svg"

[autoload]

FusionSettings="*res://addons/fusion_settings/fusion_settings.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/fusion_settings/plugin.cfg")

[fusion_settings]

config_save_path="user://settings.cfg"
autoload_path=["res://settings/"]
